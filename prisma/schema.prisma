
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"

}

model Todo {
  id            Int             @id @default(autoincrement())     
  todo          String
  status        Boolean
  price         Int
  created_at    DateTime        @default(now())
  updated_at    DateTime        @updatedAt
}
