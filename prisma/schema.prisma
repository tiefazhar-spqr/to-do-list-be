
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"

}

model Todo {
  id            String          @id @default(uuid())     
  name_todo     String
  status        Boolean
  price         Int
  created_at    DateTime        @default(now())
  updated_at    DateTime        @updatedAt
}
